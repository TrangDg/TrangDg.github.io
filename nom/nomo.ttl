  rdfs:seeAlso <https://nomenclature.info/.well-known/void> , <https://nomenclature.info/> , wd:P7749 , wd:Q64351785 ;
  dc:title "Ontologie de Nomenclature pour le catalogage des objets de musée"@fr , "Nomenclature for Museum Cataloging Ontology"@en ;
